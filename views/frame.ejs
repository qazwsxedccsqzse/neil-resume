<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title><%= title%></title>
    <script src="/js/jquery-2.1.4.min.js"></script>
    <script src="/js/bootstrap.js"></script>
    <link href="/css/bootstrap.css" rel="stylesheet" type="text/css">
    <% if(extra_links){ %>
      <% for(var index in extra_links){ %>
        <% var link = extra_links[index].split('.'); 
           var ext = link[1];
           switch(ext){
             case "css": %>
               <link href="<%= extra_links[index]%>" ref="stylesheet" type="text/css">
              <% break; %>
             <% case "js": %>
               <script src="<%= extra_links[index]%>" type="text/javascript"></script>
              <% break; %>
        <%  }
        %>
      <% } %>
    <% } %>
  </head>
  <body>
  <!-- header starts -->
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">Neil Resume</a>
      </div>
    </div>
  </nav> 
  <!-- header ends -->
  <div class="container">
    <%= body %>
  </div> 
  </body>
</html>
